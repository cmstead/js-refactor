        "mac": "cmd+shift+j l"
      },
      {
        "command": "cmstead.jsRefactor.addExport",
        "key": "ctrl+shift+j x",
        "mac": "cmd+shift+j x"
      },
      {
        "command": "cmstead.jsRefactor.shiftParams",
        "key": "ctrl+shift+j p",
        "mac": "cmd+shift+j p"
      },
      {
        "command": "cmstead.jsRefactor.markAsAsync",
