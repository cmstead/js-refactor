        "mac": "cmd+shift+j s"
      },
      {
        "command": "cmstead.jsRefactor.wrapSelection",
        "key": "ctrl+shift+j w",
        "mac": "cmd+shift+j w",
        "when": "editorHasSelection"
      }
    ],
